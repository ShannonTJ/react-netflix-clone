\r\n      .firestore()\r\n      .collection(target)\r\n      .get()\r\n      .then((snapshot) => {\r\n        const allContent = snapshot.docs.map((contentObj) => ({\r\n          ...contentObj.data(),\r\n          docId: contentObj.id,\r\n        }));\r\n        setContent(allContent);\r\n      })\r\n      .catch((error) => {\r\n        console.log(error.message);\r\n      });\r\n  }, []);\r\n\r\n  return { [target]: content };\r\n}\r\n","C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\utils\\selection-filter.js",[],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\containers\\browse.js",["188"],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\containers\\profiles.js",[],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\components\\profiles\\index.js",[],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\components\\profiles\\styles\\profiles.js",[],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\components\\loading\\index.js",[],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\components\\loading\\styles\\loading.js",[],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\components\\card\\index.js",[],"C:\\Users\\shann\\Desktop\\Web Dev Course\\FreeCodeCamp YT\\react-netflix-clone\\src\\components\\card\\styles\\card.js",[],{"ruleId":"189","replacedBy":"190"},{"ruleId":"191","replacedBy":"192"},{"ruleId":"189","replacedBy":"193"},{"ruleId":"191","replacedBy":"194"},{"ruleId":"195","severity":1,"message":"196","line":27,"column":6,"nodeType":"197","endLine":27,"endColumn":8,"suggestions":"198"},{"ruleId":"195","severity":1,"message":"199","line":26,"column":6,"nodeType":"197","endLine":26,"endColumn":8,"suggestions":"200"},{"ruleId":"195","severity":1,"message":"201","line"